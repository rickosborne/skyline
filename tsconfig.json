{
	"compilerOptions": {
		"alwaysStrict": true,
		"jsx": "react",
		"jsxFactory": "h",
		"module": "commonjs",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"sourceMap": true,
		"strict": false,
		"strictNullChecks": true,
		"target": "es5",
		"lib": [
			"ESNext"
		]
	},
	"exclude": [
		"node_modules"
	]
}
